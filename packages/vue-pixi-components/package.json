{
  "name": "vue3-pixi-components",
  "version": "1.0.1",
  "type": "module",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/src/index.d.ts",
  "license": "ISC",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/src/index.d.ts",
        "default": "./lib/index.es.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.cjs.js"
      }
    },
    "./vite": {
      "import": {
        "types": "./lib/vite/index.d.ts",
        "default": "./lib/vite.es.js"
      },
      "require": {
        "types": "./lib/vite.d.ts",
        "default": "./lib/vite.cjs.js"
      }
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "pixi.js": "^7.2.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-vue": "^0.0.0",
    "vue-tsc": "^1.4.2"
  }
}
